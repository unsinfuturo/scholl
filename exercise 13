Algoritmo sin_titulo
	
	Imprimir "Ingresar numero"
	Leer num
	
	
	Para i=num Hasta 0 Hacer
		

		
		si num > 0 Entonces
			
			num = num - 1
			
			si num mod 2 = 1 Entonces
				
				Imprimir num
				
			SiNo
				
			FinSi

		FinSi
	
		si num < 0 Entonces
			
			num = num + 1
			
			si num mod 2 = -1 Entonces
				
				Imprimir num
				
			SiNo
				
			FinSi
			
		FinSi
	FinPara
	
	
FinAlgoritmo
